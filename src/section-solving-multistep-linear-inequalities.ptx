            $c= -$a*($crit -$b);

            $left = Compute("$c");
            $comp = "&lt;";
            $right = Compute("-$a*(p-$b)");

            InequalitySetBuilder::UseVerticalSuchThat();
            Context("InequalitySetBuilder-Only");
            Context()->flags->set(formatStudentAnswer=>'parsed');
            Context()->variables->add(p=>"Real");

            $setans = Compute("{ p | p &lt; $crit}");
